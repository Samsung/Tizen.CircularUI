<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WearableUIGallery.TC.FlyoutForegroundColorTest">
    <ScrollView>
        <StackLayout>
            <Button AutomationId="GoToMain"  Text="GoToMain" Clicked="Button_Clicked_2"/>
            <Button AutomationId="Open" Text="Open" Clicked="Button_Open"/>
            <Button AutomationId="default" Text="default" Clicked="Button_Clicked"/>
            <Button AutomationId="BlueViolet" BackgroundColor="BlueViolet" Text="BlueViolet" Clicked="Button_Clicked_1"/>
            <Button AutomationId="Brown" BackgroundColor="Brown" Text="Brown" Clicked="Button_Clicked_1"/>
            <Button AutomationId="Fuchsia" BackgroundColor="Fuchsia" Text="Fuchsia" Clicked="Button_Clicked_1"/>
            <BoxView HeightRequest="50"/>
        </StackLayout>
    </ScrollView>
</ContentPage>