<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShellNavigation.Setting">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Setting Page"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Label x:Name="Current" FontSize="Small"/>
            <Button Text="GoTo Main" Clicked="OnClicked"/>
            <Button Text="Push Sub" Clicked="OnClicked2"/>
            <Button Text="GoTo Main/Sub" Clicked="OnClicked3"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>